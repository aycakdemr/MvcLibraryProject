@using Entities.Dto
@model List<BookRecordDto>
@{
    ViewBag.Title = "MembersBooks";
    Layout = "~/Views/Shared/Layout.cshtml";
}

<h2>Üye Adı: @ViewBag.dgr1 @ViewBag.dgr2</h2>

<br />
<table class="table table-danger">
    <tr>
        <th>Kitap Adı</th>
        <th>Personel</th>
        <th>Alış Tarihi</th>
        <th>İade Tarihi</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.BookName</td>
            <td>@item.StaffName</td>
            <td>@item.BuyingDate</td>
            <td>@item.GivingDate</td>
        </tr>
    }
</table>